    "url": "https://github.com/SUMANTHTPs/Playwright-snippets-vscode.git"
  },
  "bugs": "https://github.com/SUMANTHTPs/Playwright-snippets-vscode/issues",
  "icon": "images/playwright-logo.png",
  "galleryBanner": {
    "color": "#2b3137",
    "theme": "dark"
  },
